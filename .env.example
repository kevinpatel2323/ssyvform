# Database Configuration
# Use Cloud SQL connection name format for Unix socket
DATABASE_URL=postgresql://USERNAME:PASSWORD@/DATABASE_NAME?host=/cloudsql/PROJECT_ID:REGION:INSTANCE_NAME

# Or use individual connection parameters (alternative to DATABASE_URL)
# DB_HOST=/cloudsql/PROJECT_ID:REGION:INSTANCE_NAME
# DB_PORT=5432
# DB_NAME=your_database_name
# DB_USER=your_db_user
# DB_PASSWORD=your_db_password

# Google Cloud Storage
GCS_PROJECT_ID=your-gcp-project-id
GCS_PHOTOS_BUCKET=your-bucket-name

# Optional: Service Account Key JSON (if not using default credentials)
# GCS_KEY={"type":"service_account","project_id":"...","private_key":"...","client_email":"..."}

# Table Names (optional, defaults shown)
REGISTRATIONS_TABLE=registrations
ADMIN_USERS_TABLE=admin_users
